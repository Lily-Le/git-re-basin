{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: mnist-mlp",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--dataset",
                "mnist-merged",
                "--seed",
                "0",
                "--noise_type",
                "shot_noise","impulse_noise","identity","glass_blur",
            ]
        },
        {
            "name": "Python: cifar10-resnet",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--dataset",
                "cifar10-merged",
                "--gpu",
                "3080",
                "--seed",
                "0",
                "--jobtype",
                "debug",
            ]
        }
    ]
}